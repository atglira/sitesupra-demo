{
    "name": "sitesupra/sitesupra-standard",
    "description": "A standard installation of SiteSupra CMS - use it to bootstrap your project!",
    "license": "MIT",
    "authors": [
        {
            "name": "SiteSupra",
            "email": "info@sitesupra.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "doctrine/common": "2.3.*",
        "doctrine/dbal": "2.3.*",
        "doctrine/orm": "2.3.*",
        "twig/twig": "1.*",
        "twig/extensions": "1.1.*",
        "symfony/twig-bridge": "2.1.*",
        "symfony/form": "2.1.*",
        "symfony/translation": "2.1.*",
        "symfony/security": "2.1.*",
        "symfony/yaml": "2.1.*",
        "symfony/console": "2.5.*",
        "symfony/debug": "2.3.*",
        "symfony/routing": "2.1.*",
        "symfony/config": "2.1.*",
        "swiftmailer/swiftmailer": "5.2",
        "solarium/solarium": "2.4.*",
        "kriswallsmith/assetic": "1.1.*",
        "leafo/lessphp": "*",
        "pimple/pimple": "~3.0",
        "d11wtq/boris": "1.0.*",
        "maximebf/debugbar": "1.*",
        "monolog/monolog": "1.*",
        "nelmio/alice": "1.*",
	"myclabs/deep-copy": "*"
    },
    "autoload": {
        "psr-0": {
            "Supra\\": "src\\lib",
            "Sample\\": "local",
            "Supra\\Core": "lib",
            "Supra\\Package": "lib"
        },
        "classmap": ["supra/"]
    },
    "config": {
        "autoloader-suffix": "Supra"
    }
}
