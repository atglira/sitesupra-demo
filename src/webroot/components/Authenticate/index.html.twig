<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"> 
<html xmlns="http://www.w3.org/1999/xhtml"> 
<head> 
	<link rel="shortcut icon" href="/favicon.ico" type="image/vnd.microsoft.icon" /> 
	<title>Back office</title> 
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /> 
	<meta name="Generator" content="SiteSupra" />  
	<style type="text/css"> 
		html, body {
			background: #fff;
		}
	</style> 
 
</head> 
<body class="login"> 
	<div class="login_wrapper"> 
		<div class="top" style="background:#FFF"> 
			<span class="lt"><!-- --></span><span class="rt"><!-- --></span> 
		</div> 
		<div class="mid"> 
			<form id="login_form" method="post"> 
				<table width="100%" cellpadding="0" cellspacing="0"> 
					<tbody> 
						{% if message %}
						<tr class="error-message"> 
							<td colspan="2">{{message}}</td> 
						</tr> 
						{% endif %}
						<tr> 
							<td><label for="username">Email:</label></td> 
							<td><div class="input"><input AutoComplete="off" id="username" type="text" name="supra_login" value="{{email}}" /></div></td> 
						</tr> 
						<tr> 
							<td><label for="password">Password:</label></td> 
							<td><div class="input"><input AutoComplete="off" type="password" id="password" name="supra_password" value="" /></div></td> 
						</tr> 
					</tbody> 
					<tfoot> 
						<tr> 
							<td>&nbsp;</td> 
							<td> 
								<div class="button"><div><input type="submit" value="Log in" /></div></div> 
								<div class="clear"><!-- --></div> 
							</td> 
						</tr> 
					</tfoot> 
				</table> 
			</form> 
			<span class="lb"><!-- --></span><span class="rb"><!-- --></span> 
		</div> 
		<div class="logos"> 
		</div> 
	</div> 
</body> 
</html>