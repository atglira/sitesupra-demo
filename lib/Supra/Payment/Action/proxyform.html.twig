<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

		{% if autosubmit %}
		<script>
			var proxyForm = document.getElementById("proxyform");
			proxyForm.submit();
		</script>
		{% endif %}

  </head>
  <body>
		<form id="proxyform" action="{{ action }}" method="{{ method }}" >

			{% for formElement in formElements %}
			<p>... {{ formElement | raw }} ...</p>
			{% endfor %}			

			{% if ! autosubmit %}
			<input type="submit" />
			{% endif %}

		</form>
		
  </body>
</html>
