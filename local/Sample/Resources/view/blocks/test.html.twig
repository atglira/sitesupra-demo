{% set persons = collection({
    'name': 'fooBarCollection',
    'label': 'Foo Bar Collection',
    'item': set({
        'name': 'fooBarSet',
        'label': 'Foo Bar Set',
        'items': [
            property({'name': 'fullname', 'type': 'string'}),
            collection({
                'name': 'positions',
                'item': property({'name': 'position', 'type': 'string'})
            })
        ]
    })
}) %}


<h3>Persons</h3>
{% for person in persons %}
    <h5>{{ person.fullname }}</h5>
    {% for position in person.positions %}
        {{ position }}<br/>
    {% endfor %}
{% endfor %}