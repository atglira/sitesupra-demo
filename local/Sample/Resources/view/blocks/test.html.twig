{% set persons = collection({
    'name': 'fooBarCollection',
    'label': 'Foo Bar Collection',
    'item': set({
        'name': 'fooBarSet',
        'label': 'Foo Bar Set',
        'items': [
            property({'name': 'fullname', 'type': 'string', 'label': 'Fullname'}),
            collection({
                'name': 'positions',
                'item': property({'name': 'position', 'type': 'string', 'label': 'Position'})
            })
        ]
    })
}) %}


<h3>Persons</h3>
{% for person in persons %}
    <h5>{{ person.fullname }}</h5>
    {% for position in person.positions %}
        {{ position }}<br/>
    {% endfor %}
{% endfor %}

{% for keyword in property({'name': 'keywords', 'type': 'keywords'}) %}
    {{ keyword }}<br />
{% endfor %}


